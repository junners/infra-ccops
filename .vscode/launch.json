{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "pnpm start generate manifest",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229,
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Execute Command",
      "skipFiles": ["<node_internals>/**"],
      "runtimeExecutable": "node",
      "runtimeArgs": ["--loader", "ts-node/esm", "--no-warnings=ExperimentalWarning"],
      "program": "${workspaceFolder}/bin/dev.js",
      "args": ["hello", "world"]
    }
  ]
}
